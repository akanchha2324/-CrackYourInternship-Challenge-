class Solution {
public:
    void moveZeroes(vector<int>& nums) {
      int i,j,temp;
        for(i=0;i<nums.size();i++){
            for(j=i+1;j<nums.size();j++)
                {
                if(nums[i]==0 && nums[j]!=0){
                   temp= nums[i];
                    nums[i]=nums[j];
                    nums[j]=temp;
                }
                }        
    }
    }
};
